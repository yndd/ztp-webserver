    "ssh-server": {
      "network-instance": [
        {
          "name": "mgmt",
          "admin-state": "enable"
        }
      ]
    },
    "dns": {
      "network-instance": "mgmt",
      "server-list": [
        {{ $x := jsonstringify .DnsServers}}{{ join $x ",\n" }}
      ]
    },
    "ntp": {
      "admin-state": "enable",
      "network-instance": "mgmt",
      "server": [{{range $index, $value := .NtpServers}}{{if $index}},{{end}}
        {
          "address": "{{ $value }}"
        }{{end}}
      ]
    },
    "srl_nokia-lldp:lldp": {
      "admin-state": "enable"
    }
  },
  "network-instance": [
    {
      "name": "mgmt",
      "type": "ip-vrf",
      "admin-state": "enable",
      "description": "Management network instance",
      "interface": [
        {
          "name": "mgmt0.0"
        }
      ],
      "protocols": {
        "linux": {
          "import-routes": true,
          "export-routes": true,
          "export-neighbors": true
        }
      },
      "static-routes": {
        "route": [
          {
            "prefix": "0.0.0.0/0",
            "next-hop-group": "mgmt-net-router"
          }
        ]
      },
      "next-hop-groups": {
        "group": [
          {
            "name": "mgmt-net-router",
            "nexthop": [
              {
                "index": 1,
                "ip-address": "{{.GatewayIp}}",
                "admin-state": "enable"
              }
            ]
          }
        ]
      }
    }
  ]
}