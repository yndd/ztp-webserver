  "srl_nokia-interfaces:interface": [
    {
      "name": "mgmt0",
      "admin-state": "enable",
      "subinterface": [
        {
          "index": 0,
          "admin-state": "enable",
          "ipv4": {
            "address": [
              {
                "ip-prefix": "{{.Cidr}}"
              }
            ]
          }
        }
      ]
    }
  ],
